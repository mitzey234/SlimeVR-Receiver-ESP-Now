{
    "build": {
      "arduino": {
        "ldscript": "eagle.flash.4m.ld"
      },
      "core": "esp8266",
      "extra_flags": [
        "-DARDUINO_ESP8266_WEMOS_D1MINI",
        "-DESP8266"
      ],
      "f_cpu": "80000000L",
      "f_flash": "40000000L",
      "flash_mode": "dio",
      "mcu": "esp8266",
      "variant": "d1_mini"
    },
    "connectivity": [
      "wifi"
    ],
    "frameworks": [
      "arduino",
      "esp8266-rtos-sdk",
      "esp8266-nonos-sdk"
    ],
    "name": "WEMOS D1 Mini (ESP8266)",
    "upload": {
      "flash_size": "4MB",
      "maximum_ram_size": 81920,
      "maximum_size": 4194304,
      "resetmethod": "nodemcu",
      "require_upload_port": true,
      "speed": 921600
    },
    "url": "https://www.wemos.cc/en/latest/d1/d1_mini.html",
    "vendor": "WEMOS"
  }
  